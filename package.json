		"dateformat": "^5.0.3",
		"eslint": "^8.53.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "^2.34.1",
		"prettier": "^3.0.3",
		"prettier-plugin-svelte": "^3.0.3",
		"sass": "^1.69.5",
		"striptags": "^3.2.0",
		"svelte": "^4.2.2",
		"svelte-check": "^3.5.2",
		"svelte-sitemap": "^2.5.4",
		"typescript": "^5.2.2",
